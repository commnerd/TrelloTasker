{
    "name": "commnerd/trello-tasker",
    "description": "Library for handling Trello task management",
    "type": "library",
    "authors": [
        {
            "name": "Michael J. Miller",
            "email": "commnerd@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "../Tasker"
        }
    ],
    "require": {
        "commnerd/tasker": "*"
    },
    "autoload": {
        "psr-4": {
            "TrelloTasker\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "mashape/unirest-php": "3.*",
        "vlucas/phpdotenv": "^5.1@dev"
    }
}
